<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.betagro.adulvitc.aquaculturewatertest">

    <application
        android:allowBackup="true"
        android:icon="@drawable/do_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login1Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistryActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Detail2Activity"
            android:label="Water Quality Parameter"
            android:parentActivityName=".DetailActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".DetailActivity"
            android:label="Detail of Water Quality"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".PieChartActivity"></activity>
    </application>

</manifest>